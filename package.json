    "lint": "eslint --ignore-path .gitignore \"**/*.+(js|jsx)\""
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.3.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.0",
    "postcss": "^8.3.7",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.15",
    "typescript": "^4.3.2",
    "vite": "^2.5.4"
  }
}
